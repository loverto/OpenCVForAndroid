<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.yinlongfei.opencv">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:screenOrientation="landscape">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <!-- 目标检测Activity -->
        <activity
            android:name=".ObjectDetectingActivity"
            android:screenOrientation="landscape"
            android:theme="@style/FullscreenTheme" />

        <activity android:name=".FdActivity"></activity>
        <activity android:name=".FaceDetectingActivity"></activity>
        <activity android:name=".DnnActivity"></activity>
    </application>

</manifest>